{
  "name": "universal",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngc:aot": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "ngc:uni": "node_modules/.bin/ngc -p tsconfig-uni.json",
    "build:aot": "webpack --config webpack.config.aot.js",
    "serve:aot": "lite-server -c=bs-config.aot.js",
    "build:uni": "webpack --config webpack.config.uni.js",
    "serve:uni": "node src/dist/server.js",
    "uni": "npm run ngc:aot && npm run build:aot && npm run ngc:uni && npm run build:uni && npm run serve:uni",
    "aot": "npm run ngc:aot && npm run build:aot && npm run serve:uni",
    "univ": "npm run build:uni & npm run serve:uni"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.1.1",
    "@angular/common": "^4.1.1",
    "@angular/compiler": "^4.1.1",
